/* pages/search/search.wxss */
page{
  background-color: #f5f5f5;
}

.searchback{
  display: flex;
  text-align: center;
  padding-top: 50rpx;
  background-color: #00E0FF;
  height: 150rpx;
  border-bottom-left-radius: 20%;
  border-bottom-right-radius: 20%;
  overflow: hidden;
}
.block1{
background-color:#74F9FF;
border-radius: 60%;
width: 300rpx;
height: 300rpx;
z-index: 0;
top: -120rpx;
left: -100rpx;
position: absolute;
}
.block2{
  background-color:#74F9FF;
border-radius: 100%;
width: 400rpx;
height: 400rpx;
z-index: 0;
top: -250rpx;
left: 500rpx;
position: absolute;
}
.block3{
  background-color:#74F9FF;
border-radius: 100%;
width: 200rpx;
height: 100rpx;
z-index: 0;
top: 30rpx;
left: 300rpx;
position: absolute;
}
.searchBox{
  display: flex;
  margin-left: 70rpx;
  border-radius: 50rpx;
  background-color: #f5f5f5;
  height: 100rpx;
  z-index: 1;
}
.searchIcon{
  margin: 10rpx;
  padding-left: 10rpx;
  margin-top: 30rpx;
}
.searcher{
  width:40rpx;
  height:40rpx;
}
.searchIt{
  width: 550rpx;
  margin-top: 30rpx;
}